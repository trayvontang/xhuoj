
* { font-family: Microsoft YaHei, Helvetica, Arial, sans-serif }
body { background-color: #FFFFFF; font-size: 10pt }
td { background-color: #FFFFFF; font-size: 10pt }
/* Table colors */
.TableHeadingColor     { background: #CCCCFF } /* Dark mauve */
.TableSubHeadingColor  { background: #EEEEFF } /* Light mauve */
.TableRowColor         { background: #FFFFFF } /* White */
/* Font used in left-hand frame lists */
.FrameTitleFont   { font-size: 10pt}
.FrameHeadingFont { font-size: 10pt }
.FrameItemFont    { font-size: 10pt }
/* Links */
a:link { color: #0000FF; text-decoration: none;}
a:visited { color: #800080; text-decoration: none;}
a:active { color: #FF0000; text-decoration: underline;}
a:hover { color: #FF0000; text-decoration: underline;}
/* Navigation bar fonts and colors */
.NavBarCell1    { background-color:#EEEEFF;} /* Light mauve */
.NavBarCell1Rev { background-color:#00008B;} /* Dark Blue */
.NavBarFont1    { color:#000000;}
.NavBarFont1Rev { color:#FFFFFF;}
.NavBarCell2    { background-color:#FFFFFF;}
.NavBarCell3    { background-color:#FFFFFF;}
